<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--流样式布局自定义属性-->
    <declare-styleable name="FlowLayout">
        <attr name="lineSpacing" format="dimension" />
        <attr name="maxLine" format="integer" />
    </declare-styleable>

    <declare-styleable name="CarLayout">
        <attr name="carIcon" format="reference" />
        <attr name="carTextColor" format="color" />
        <attr name="carTextSize" format="integer" />
        <attr name="carTopMargin" format="dimension" />
    </declare-styleable>
</resources>